{
    "name": "mpyw/scoped-auth",
    "description": "Apply specific scope for user authentication.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "illuminate", "auth", "scope"],
    "autoload": {
        "psr-4": {
            "Mpyw\\ScopedAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\ScopedAuth\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/auth": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/database": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/contracts": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": "^4.9 || ^5.9 || >=6.6",
        "phpunit/phpunit": ">=9.5",
        "mockery/mockery": "^1.3.3 || ^1.4.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mpyw\\ScopedAuth\\ScopedAuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
