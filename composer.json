{
    "name": "mpyw/scoped-auth",
    "description": "Apply specific scope for user authentication.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "illuminate", "auth", "scope"],
    "autoload": {
        "psr-4": {
            "Mpyw\\ScopedAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\ScopedAuth\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/auth": "^11.0 || ^12.0 || ^13.0",
        "illuminate/database": "^11.0 || ^12.0 || ^13.0",
        "illuminate/contracts": "^11.0 || ^12.0 || ^13.0",
        "illuminate/support": "^11.0 || ^12.0 || ^13.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": ">=9.0",
        "phpunit/phpunit": ">=11.0",
        "mockery/mockery": "^1.6.12"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mpyw\\ScopedAuth\\ScopedAuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
