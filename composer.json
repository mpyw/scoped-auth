{
    "name": "mpyw/scoped-auth",
    "description": "Apply specific scope for user authentication.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "illuminate", "auth", "scope"],
    "autoload": {
        "psr-4": {
            "Mpyw\\ScopedAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\ScopedAuth\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "illuminate/auth": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/database": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/contracts": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "php-coveralls/php-coveralls": "^2.1",
        "mockery/mockery": "^1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpyw\\ScopedAuth\\ScopedAuthServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
